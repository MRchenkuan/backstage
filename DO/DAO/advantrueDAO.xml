<?xml version="1.0" encoding="UTF-8"?>
<mapper>
    <update id="updateItemByInfo" >
        update T_ADVANTCE set #{setting} where id="#{id}";
    </update>

    <select id="getALlItems">
        select
        id,
        img_url,
        img_thumb,
        img_title,
        img_desc
        from T_ADVANTCE where id like "#{type}%"
    </select>
</mapper>